//if you don't know what a parameter does, visit:
//https://developer.valvesoftware.com/wiki/$phong
"VertexLitGeneric"
{
    "$basetexture" "${EXPORTPATH}\${ALBEDONAME}"
    "$bumpmap" "${EXPORTPATH}\${NORMALNAME}"
    "$phongexponenttexture" "${EXPORTPATH}\${EXPONENTNAME}"

    "$color2" "[0 0 0]"
    "$blendtintbybasealpha" "1"
    "$phong" "1"
    "$phongboost" "1"                       //larger values create a more intense highlight, mess with it
    "$phongalbedotint" "0"                  //try setting this to 1 and check if it looks ok
    "$phongfresnelranges" "[1 0.5 0]"       //this isn't good for all material setups. glossy surfaces likely need higher 2nd and 3rd numbers.

    "$basemapalphaenvmapmask" "1"
    "$envmapfresnel" "1"

    //DEBUG
    //"$envmap" "env_cubemap"
    //"$envmaptint" "[1 1 1]"
    //DEBUG

    "Proxies"
    {
        "MwEnvMapTint"
        {
            "specularity" "0.5"             //0.25, 0.5, 0.75, 1
            "min" "0"
            "max" "1"                       //change this if too bright in game
        }
    }
}