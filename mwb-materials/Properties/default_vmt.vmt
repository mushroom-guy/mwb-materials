"VertexLitGeneric"
{
    "$basetexture" "${EXPORTPATH}\${ALBEDONAME}"
    "$bumpmap" "${EXPORTPATH}\${NORMALNAME}"
    "$phongexponenttexture" "${EXPORTPATH}\${EXPONENTNAME}"

    /////////////////
    "$color2" "[0 0 0]"                                     //do not touch this
    "$blendtintbybasealpha" "1"                             //do not touch this
    /////////////////

    "$phong" "1"
    "$phongboost" "1"
    "$phongalbedotint" "1"
    "$phongalbedoboost" "10"                                //change this if not bright enough
    "$phongfresnelranges" "[1 0.1 0]"

    //rimlight doesn't properly show in hlmv, make sure you're changing these values in game
    "$rimlight" "1"
    "$rimlightexponent" "100"
    "$rimmask" "1"
    "$rimlightboost" "1"
       
    /////////////////
    "$normalmapalphaenvmapmask" "1"                         //do not touch this
    /////////////////

    "$envmap" "env_cubemap"
    "$envmaptint" "[0.1 0.1 0.1]"                           //change this to match albedo's color if metallic

    //remove this if not using MWB
    "Proxies"
    {
        "MwEnvMapTint"
        {
            "min" "0"
            "max" "0.1"                                     //change this if too bright in game
        }
    }
}